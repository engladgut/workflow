    "express": "^4.17.1",
    "install": "^0.13.0",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "mathjs": "^6.2.3",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "npm": "^6.13.0",
    "react": "^16.11.0",
    "react-d3-graph": "^2.3.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
